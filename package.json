{
  "name": "twitchbot",
  "version": "2.0.0",
  "description": "birdofchess twitch bot",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open",
    "build:bytes": "NODE_ENV=production webpack --config=./webpack.config.js",
    "bot": "nodemon src/bot --exec babel-node --extensions \".ts,.js\"",
    "bot:dev": "npm run bot",
    "bot:prod": "rm -rf dist/ && NODE_ENV=production npx babel src --extensions \".ts,.js\" --out-dir dist && NODE_ENV=production forever dist/bot",
    "server": "rm -rf dist/ && npx babel src --extensions \".ts,.js\" --out-dir dist && cp src/server/index.html dist/server/index.html && nodemon dist/server",
    "server:dev": "npm run server",
    "server:prod": "rm -rf dist/ && NODE_ENV=production npx babel src --extensions \".ts,.js\" --out-dir dist && cp src/server/index.html dist/server/index.html && NODE_ENV=production forever dist/server"
  },
  "author": "pavasich",
  "license": "ISC",
  "repository": "https://github.com/pavasich/twitchbot",
  "dependencies": {
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.4",
    "babel-runtime": "^6.26.0",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "graceful-fs": "^4.1.11",
    "html-webpack-plugin": "^5.5.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "4.17.15",
    "lowdb": "^1.0.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-fetch": "^1.7.3",
    "postcss-loader": "^3.0.0",
    "postcss-reporter": "^6.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-draggable": "^3.0.5",
    "react-hot-loader": "^4.3.11",
    "react-redux": "^5.0.7",
    "redux-saga": "^0.16.0",
    "twitch": "^4.3.8",
    "twitch-auth": "^4.3.8",
    "twitch-chat-client": "^4.3.8",
    "twitch-js": "^2.0.0-beta.43",
    "twitch-pubsub-client": "^4.3.8",
    "twitch-webhooks": "^4.3.8",
    "url-loader": "^1.1.1",
    "validator": "^10.8.0",
    "webpack": "^4.5.0",
    "webpack-dev-server": "^3.1.11",
    "webpack-merge": "^4.1.4",
    "whatwg-fetch": "^2.0.3",
    "write-file-webpack-plugin": "^4.4.1",
    "ws": "3.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.20.7",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.112",
    "@types/lowdb": "^1.0.4",
    "@types/mini-css-extract-plugin": "^0.8.0",
    "@types/node": "^10.5.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "typescript": "^2.9.2",
    "webpack-cli": "^3.1.1"
  }
}
