{
  "name": "twitchbot",
  "version": "2.0.0",
  "description": "birdofchess twitch bot",
  "main": "index.js",
  "scripts": {
    "bot": "nodemon src/bot --exec babel-node --extensions \".ts,.js\"",
    "bot:dev": "npm run bot",
    "bot:prod": "rm -rf dist/ && NODE_ENV=production npx babel src --extensions \".ts,.js\" --out-dir dist && NODE_ENV=production forever dist/bot",
    "server": "rm -rf dist/ && npx babel src --extensions \".ts,.js\" --out-dir dist && cp src/server/index.html dist/server/index.html && nodemon dist/server",
    "server:dev": "npm run server",
    "server:prod": "rm -rf dist/ && NODE_ENV=production npx babel src --extensions \".ts,.js\" --out-dir dist && cp src/server/index.html dist/server/index.html && NODE_ENV=production forever dist/server"
  },
  "author": "pavasich",
  "license": "ISC",
  "repository": "https://github.com/pavasich/twitchbot",
  "dependencies": {
    "@types/lodash": "^4.14.112",
    "express": "^4.16.3",
    "isomorphic-fetch": "^2.2.1",
    "lowdb": "^1.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "twitch-js": "^1.2.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/node": "^7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.54",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.54",
    "@babel/polyfill": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-typescript": "^7.0.0-beta.54",
    "@babel/runtime": "^7.0.0-beta.54",
    "@types/express": "^4.16.0",
    "@types/lowdb": "^1.0.4",
    "@types/node": "^10.5.2",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "typescript": "^2.9.2"
  }
}
